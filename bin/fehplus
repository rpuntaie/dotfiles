#!/usr/bin/env bash

feh --draw-exif -r -G -d -F \
    --action1 'echo %F | xclip -i' \
    --action2 ';mogrify -strip -interlace Plane -gaussian-blur 0.05 -quality 30 %F' \
    --action3 ';mogrify -rotate 90 %F' \
    --action4 ';mogrify -rotate -90 %F' \
    --action5 'rm %F' \
    --action6 'cp %F '"$1"'' \
    --action  'mv %F '"$1"'' .
