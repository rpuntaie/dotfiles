#!/usr/bin/env sh

cd ~
stow --ignore=home --no-folding -R -t .local dotfiles
#chmod 700 .local/var/lib/gnupg
chmod -R 700 .local
cd dotfiles 
stow home

#see user-dirs.dirs
mkdir -p $HOME/Desktop
mkdir -p $HOME/Downloads
mkdir -p $HOME/.Templates
mkdir -p $HOME/Public
mkdir -p $HOME/Documents
mkdir -p $HOME/Music
mkdir -p $HOME/Media
mkdir -p $HOME/Media
